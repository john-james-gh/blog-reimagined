{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "targetDefaults": {
    "build": {
      "cache": true,
      "outputs": ["{projectRoot}/.next/**", "!{projectRoot}/.next/cache/**"]
    },
    "lint": {
      "cache": true
    },
    "typegen": {
      "cache": true
    },
    "test": {
      "cache": true,
      "outputs": ["{projectRoot}/coverage"]
    }
  },
  "tui": {
    "autoExit": false
  },
  "useDaemonProcess": false
}
